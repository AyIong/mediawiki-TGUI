{
  "skin-theme": {
    "options": ["day", "night", "os"],
    "preferenceKey": "tgui-theme"
  },
  "tgui-feature-blur": {
    "options": ["enabled", "disabled", "full"],
    "preferenceKey": "tgui-blur"
  },
  "tgui-feature-reduced-motion": {
    "options": ["enabled", "disabled"],
    "preferenceKey": "tgui-motion"
  },
  "tgui-feature-darkened-images": {
    "options": ["disabled", "light", "medium"],
    "preferenceKey": "tgui-darkened-images"
  },
  "tgui-feature-primary-hue-slider": {
    "defaultValue": "210",
    "min": 0,
    "max": 360,
    "step": 5,
    "type": "range",
    "preferenceKey": "primary-hue"
  }
}
